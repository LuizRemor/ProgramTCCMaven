package br.com.eng.laje;

import java.io.IOException;

import javafx.fxml.FXML;
import javafx.scene.Group;
import javafx.scene.shape.Polyline;

public class SecondaryController {
	
	PrimaryController primaryController;
	
	@FXML
	Group groupYNegativo = new Group();
	
	@FXML
	Group groupXNegativo = new Group();
	
	@FXML
	Group groupYLinhaNegativo = new Group();
	
	@FXML
	Group groupXLinhaNegativo = new Group();

	@FXML
	private void btRetornaTelaDeCalculos() throws IOException {
		
		App.setRoot("primary");
		
	}
	
	public void montaAcoNegativo() {
		
		Polyline polyline = new Polyline();
		
		polyline.getPoints().addAll(new Double[]{        
	    	         200.0, 50.0, 
	    	         220.0, 50.0, 
	    	         220.0, 100.0,          
	    	         220.0, 150.0,
	    	         200.0, 150.0});
		
		groupYNegativo = new Group(polyline);
		
	}
	
	
	
	
}