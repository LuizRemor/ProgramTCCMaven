package br.com.eng.laje;

import java.io.IOException;

import javafx.fxml.FXML;
import javafx.scene.shape.Polyline;

public class SecondaryController {
	
	PrimaryController primaryController;
	
	Polyline polylineYNegativo = new Polyline();
	Polyline polylineXNegativo = new Polyline();
	Polyline polylineYLinhaNegativo = new Polyline();
	Polyline polylineXLinhaNegativo = new Polyline();

	@FXML
	private void btRetornaTelaDeCalculos() throws IOException {
		
		App.setRoot("primary");
		
	}
	
	public void montaAcoNegativo() {
		
		polylineYNegativo.getPoints().addAll(new Double[]{        
	    	         200.0, 50.0, 
	    	         220.0, 50.0, 
	    	         220.0, 100.0,          
	    	         220.0, 150.0,
	    	         200.0, 150.0});
		
	}
	
	
	
	
}